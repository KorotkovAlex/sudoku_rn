{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-react",
        "tslint-config-prettier",
        "tslint-react-hooks"
    ],
    "jsRules": {},
    "rules": {
        "react-hooks-nesting": "error",
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "jsx-no-lambda": false,
        "jsx-boolean-value": false,
        "no-any": false,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": false,
        "indent": [
            true,
            "spaces",
            4
        ],
        "max-line-length": [
            true,
            100
        ],
        "no-trailing-whitespace": true,
        "quotemark": [
            true,
            "double",
            "avoid-escape"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "member-access": false,
        "no-var-requires": true,
        "prefer-const": false
    },
    "rulesDirectory": []
}